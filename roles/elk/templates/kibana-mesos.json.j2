{
  "name": "kibana-mesos",
  "config": {
    "kibana-mesos": {
      "id": "{{ kibana_mesos_id }}",
      "framework-name": "{{ kibana_mesos_framework_name }}",
      "scheduler": {
        "cpu": {{ kibana_mesos_scheduler_cpu }},
        "ram": {{ kibana_mesos_scheduler_ram }}
      },
      "executor": {
        "image": "{{ kibana_mesos_image }}",
        "cpu": {{ kibana_mesos_executor_cpu }},
        "ram": {{ kibana_mesos_executor_ram }},
        "instances": {{ kibana_mesos_instances }},
        "command": "export ELASTICSEARCH_SERVICE={{ kibana_mesos_elasticsearch_service }}; export KIBANA_SERVICE={{ kibana_mesos_kibana_service }}; export CONSUL_SSL={{ do_consul_ssl|to_nice_json }}; export CONSUL_SSL_VERIFY=false; tini -s -- /launch.sh"
      }
    }
  }
}